

@model IEnumerable<task.Models.ReceivingSide>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table table-hover table-active">
    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.name</td>
        </tr>
    }

</table>